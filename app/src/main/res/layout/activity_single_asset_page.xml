<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="top"
    android:gravity="top"
    android:orientation="vertical"
    tools:context=".SingleAssetPage">

    <ImageView
        android:id="@+id/CompanyMastHead"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@drawable/rectangle_design_element" />

    <TableRow
        android:id="@+id/row1"
        android:layout_below="@+id/CompanyMastHead"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:background="@color/CompanyColorBeta">
        <Button
            android:id="@+id/prevAsset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Prevt"
            android:textColor="@color/CompanyColorDelta"
            android:background="@drawable/landingpagebutton"
            android:layout_margin="8dp"
            android:layout_column="0"/>
        <Spinner
            android:id="@+id/AssetName"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_column="1"
            android:background="@drawable/single_asset_page_textview"
            android:gravity="center"
            android:textAlignment="center"
            />
        <Button
            android:id="@+id/nextAsset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Next"
            android:textColor="@color/CompanyColorDelta"
            android:background="@drawable/landingpagebutton"
            android:layout_margin="8dp"
            android:layout_column="2"/>
    </TableRow>

    <TableRow
        android:id="@+id/row2"
        android:layout_below="@+id/row1"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:background="@color/CompanyColorBeta">

        <TextView
            android:id="@+id/displayLatitude"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:text="Latitude"
            android:textSize="18sp"
            android:gravity="center"
            android:background="@drawable/single_asset_page_textview"
            android:layout_margin="5dp"
            android:maxLines="1"
            android:layout_column="0"/>

        <ImageButton
            android:id="@+id/mapsButton"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_column="1"
            android:background="@drawable/landingpagebutton"
            android:scaleType="fitCenter"
            android:src="@drawable/locate_png" />

        <TextView
            android:id="@+id/displayLongitude"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:text="Longitude"
            android:textSize="18sp"
            android:gravity="center"
            android:layout_gravity="center"
            android:background="@drawable/single_asset_page_textview"
            android:layout_margin="5dp"
            android:maxLines="1"
            android:layout_column="2"/>

    </TableRow>

